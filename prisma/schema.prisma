
datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"  
}

model Project {
  id            String   @id @default(cuid())
  name          String
  description   String
  datasource    String
  folderId      String?
  accessToken   String?
  refreshToken  String?
  tokenExpiry   DateTime?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
