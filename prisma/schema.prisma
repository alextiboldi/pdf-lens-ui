
datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"  
}

model Project {
  id          String   @id @default(cuid())
  name        String
  description String
  datasource  String
  folderId    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
